{
  "name": "@cfworker/cosmos",
  "version": "4.0.3",
  "description": "Azure Cosmos DB client for Cloudflare Workers and service workers",
  "keywords": [
    "azure",
    "cosmos",
    "cosmosdb",
    "cosmos-db",
    "cloudflare",
    "worker",
    "workers",
    "service-worker"
  ],
  "repository": "https://github.com/cfworker/cfworker",
  "author": "Jeremy Danyow <jdanyow@gmail.com>",
  "homepage": "https://github.com/cfworker/cfworker/tree/master/packages/cosmos/README.md",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "node ../../patch.mjs && tshy",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "pretest": "esbuild test/test.ts --target=esnext --bundle --format=esm --conditions=worker,browser --outdir=dist-test --ignore-annotations",
    "test": "node ../../test.mjs"
  },
  "dependencies": {
    "rfc4648": "^1.5.3"
  },
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "chai": "^5.1.2",
    "esbuild": "^0.24.0",
    "mocha": "^11.0.1",
    "tshy": "^3.0.2",
    "typescript": "^5.7.2",
    "wrangler": "^3.92.0"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  }
}
