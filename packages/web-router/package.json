{
  "name": "@cfworker/web-router",
  "version": "1.1.0",
  "description": "A router for Cloudflare Workers or service workers using path-to-regexp",
  "keywords": [
    "middleware",
    "router",
    "cloudflare",
    "worker",
    "workers",
    "service-worker"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "tsconfig.json",
    "README.md",
    "package.json"
  ],
  "repository": "https://github.com/cfworker/cfworker",
  "author": "Jeremy Danyow <jdanyow@gmail.com>",
  "homepage": "https://github.com/cfworker/cfworker/tree/master/packages/web-router/README.md",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "test": "cfworker test test/**/*.spec.ts --nocheck"
  },
  "dependencies": {
    "@cfworker/http-errors": "^1.0.11",
    "@cfworker/web": "^1.1.0",
    "path-to-regexp": "^6.1.0"
  },
  "devDependencies": {
    "typescript": "^3.8.3"
  }
}
