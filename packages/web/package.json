{
  "name": "@cfworker/web",
  "version": "1.8.4",
  "description": "Web framework for Cloudflare Workers and service workers, inspired by Koa and fastify",
  "keywords": [
    "middleware",
    "cloudflare",
    "worker",
    "workers",
    "service-worker"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "tsconfig.json",
    "README.md",
    "package.json"
  ],
  "repository": "https://github.com/cfworker/cfworker",
  "author": "Jeremy Danyow <jdanyow@gmail.com>",
  "homepage": "https://github.com/cfworker/cfworker/tree/master/packages/web/README.md",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "test": "cfworker test test/**/*.spec.ts --nocheck"
  },
  "dependencies": {
    "@cfworker/json-schema": "^1.8.3",
    "@cfworker/worker-types": "^1.8.0",
    "@cloudflare/workers-types": "^2.1.0",
    "@types/cookie": "^0.4.0",
    "@types/escape-html": "^1.0.0",
    "@types/json-pointer": "^1.0.30",
    "@types/koa-compose": "^3.2.5",
    "@types/secure-json-parse": "^1.0.2",
    "@types/statuses": "^1.5.0",
    "cookie": "^0.4.1",
    "escape-html": "^1.0.3",
    "koa-compose": "^4.1.0",
    "negotiator": "^0.6.2",
    "path-to-regexp": "^6.2.0",
    "secure-json-parse": "^2.3.2",
    "statuses": "^1.5.0"
  },
  "devDependencies": {
    "@cfworker/dev": "^1.8.3",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "json-pointer": "^0.6.1",
    "mocha": "^8.3.2",
    "typescript": "^4.2.3"
  }
}
